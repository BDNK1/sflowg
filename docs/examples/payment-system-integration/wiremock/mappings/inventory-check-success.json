{
  "name": "Inventory Check - Item Available",
  "request": {
    "method": "GET",
    "urlPath": "/inventory",
    "queryParameters": {
      "item_name": {
        "matches": ".*"
      },
      "quantity": {
        "matches": "[0-9]+"
      }
    },
    "headers": {
      "Authorization": {
        "matches": "Bearer .*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json",
      "X-Response-Time": "23ms"
    },
    "jsonBody": {
      "available": true,
      "item_name": "{{request.query.item_name}}",
      "quantity_available": 1000,
      "quantity_requested": "{{request.query.quantity}}",
      "warehouse": "MAIN-US-EAST",
      "estimated_ship_date": "2024-11-08"
    }
  }
}

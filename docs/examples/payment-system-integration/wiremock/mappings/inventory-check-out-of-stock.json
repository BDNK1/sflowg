{
  "name": "Inventory Check - Out of Stock",
  "priority": 2,
  "request": {
    "method": "GET",
    "urlPath": "/inventory",
    "queryParameters": {
      "item_name": {
        "equalTo": "Out of Stock Item"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "available": false,
      "item_name": "Out of Stock Item",
      "quantity_available": 0,
      "quantity_requested": "{{request.query.quantity}}",
      "warehouse": null,
      "estimated_restock_date": "2024-11-15"
    }
  }
}
